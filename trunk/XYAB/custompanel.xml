<?xml version='1.0' encoding='UTF-8'?>
<pyvcp>

    <labelframe text="Mode">
        <font>("Helvetica",10)</font>
        <hbox>
    	    <button>
    		    <font>("Helvetica",14)</font>
    		    <width>3</width>
    		    <halpin>"world"</halpin>
                <text>"World"</text>
            </button>
            <button>
                <font>("Helvetica",14)</font>
                <width>3</width>
                <halpin>"joint"</halpin>
                <text>"Joint"</text>
            </button>
        </hbox>
    </labelframe>

    <labelframe text="Jogspeed">
        <font>("Helvetica",10)</font>
        <scale>
            <font>("Helvetica",16)</font>
            <width>"25"</width>
            <halpin>"jogspeed"</halpin>
            <resolution>10</resolution>
            <orient>HORIZONTAL</orient>
            <initval>100</initval>
            <min_>10</min_>
            <max_>500</max_>
        </scale>
    </labelframe>

<tabs>
    <font>("Helvetica",14)</font>
    <names>["Joint", "World"]</names>
    <vbox>
      <hbox>
        <labelframe text="Jog 0">
             <vbox>
                <hbox>
                 <boxfill fill="none"/>
                 <button>
                    <font>("Helvetica",16)</font>
                    <width>3</width>
                    <halpin>"jog0-"</halpin>
                    <text>"-"</text>
                 </button>
                 <label>
                    <width>2</width>
                    <text>" "</text>
                    <font>("Helvetica",20)</font>
                 </label>                 
                </hbox>
                <boxfill fill="none"/>
                <boxanchor anchor="e"/>
                <button>
                    <font>("Helvetica",16)</font>
                    <width>3</width>
                    <halpin>"jog0+"</halpin>
                    <text>"+"</text>
                </button>
             </vbox>
        </labelframe>
        <labelframe text="Jog 1">
             <vbox>
                <hbox>
                 <boxfill fill="none"/>
                 <label>
                    <width>2</width>
                    <text>" "</text>
                    <font>("Helvetica",20)</font>
                 </label>                 
                 <button>
                    <font>("Helvetica",16)</font>
                    <width>3</width>
                    <halpin>"jog1-"</halpin>
                    <text>"-"</text>
                 </button>
                </hbox>
                <boxfill fill="none"/>
                <boxanchor anchor="w"/>
                 <button>
                    <font>("Helvetica",16)</font>
                    <width>3</width>
                    <halpin>"jog1+"</halpin>
                    <text>"+"</text>
                </button>
             </vbox>
        </labelframe>
      </hbox>
      <hbox>
        <labelframe text="Jog 3">
             <vbox>
                <hbox>                 <label>
                    <width>2</width>
                    <text>" "</text>
                    <font>("Helvetica",20)</font>
                 </label>                 

                 <boxfill fill="none"/>
                 <button>
                    <font>("Helvetica",16)</font>
                    <width>3</width>
                    <halpin>"jog3-"</halpin>
                    <text>"-"</text>
                 </button>
                 <label>
                    <width>2</width>
                    <text>" "</text>
                    <font>("Helvetica",20)</font>
                 </label>                 
                </hbox>
                <boxfill fill="none"/>
                <boxanchor anchor="e"/>
                <button>
                    <font>("Helvetica",16)</font>
                    <width>3</width>
                    <halpin>"jog3+"</halpin>
                    <text>"+"</text>
                </button>
             </vbox>
        </labelframe>
        <labelframe text="Jog 4">
             <vbox>
                <hbox>
                 <boxfill fill="none"/>
                 <label>
                    <width>2</width>
                    <text>" "</text>
                    <font>("Helvetica",20)</font>
                 </label>                 
                 <button>
                    <font>("Helvetica",16)</font>
                    <width>3</width>
                    <halpin>"jog4-"</halpin>
                    <text>"-"</text>
                 </button>
                </hbox>
                <boxfill fill="none"/>
                <boxanchor anchor="w"/>
                 <button>
                    <font>("Helvetica",16)</font>
                    <width>3</width>
                    <halpin>"jog4+"</halpin>
